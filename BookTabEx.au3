#NoTrayIcon
Opt('MustDeclareVars', 1)
Opt('ExpandVarStrings', 1)

#include <.\lib\Aut2Log.au3>
#include <GDIPlus.au3>
#include <GuiConstantsEx.au3>
#include <GuiListView.au3>
#include <GuiStatusBar.au3>
#include <.\lib\MemoryDll.au3>

Local $sActiveFolder = '@LocalAppDataDir@\duDat\BooktabZ\'

If FileExists($sActiveFolder) = 0 Then
	MsgBox(48, 'Errore', "BookTabZ non risulta installato")
	_Aut2Log_Write('BookTabZ not installed', $__A2L_TYPE_MESSAGE)
	Exit
EndIf

Global Const $BASS_SAMPLE_LOOP = 4, $BASS_UNICODE = 0x80000000, $WM_NCHITTEST = 0x0084, $HTCAPTION = 2
Global $sBookTitle, $iSelectedItems, $aButton[6], $aStatusBarParts[3] = [250, 450, -1], $iCurrentBook, $iCurr_Average

If $CmdLine[0] <> 0 Then _Aut2Log_Settings($CmdLine[1] And '-l', $__A2L_OUT_FILE) ; If 1st parameter is -l then enable logging
Local $EnvInfo = _Aut2Log_Environment()
_Aut2Log_Write($EnvInfo, $__A2L_TYPE_ENVINFO, False, $__A2L_OUT_FILE)
Global $aProfileDirList = __FileListToArray($sActiveFolder, '*', 2)

Local $aProfiles[0]
For $i = 0 To UBound($aProfileDirList) - 1
	If StringLen($aProfileDirList[$i]) > 12 And Not StringInStr($aProfileDirList[$i], 'common') Then
		ReDim $aProfiles[UBound($aProfiles) + 1]
		$aProfiles[UBound($aProfiles) - 1] = $aProfileDirList[$i]
	EndIf
Next

If UBound($aProfiles) = 0 Then
	MsgBox(48, 'Errore', "Nessun profilo rilevato.@CRLF@Accertarsi di aver eseguito l'accesso in BookTabZ")
	_Aut2Log_Write('Not logged in', $__A2L_TYPE_MESSAGE)
	Exit
ElseIf UBound($aProfiles) = 1 Then
	Global $sProfileDir = $sActiveFolder & $aProfiles[0]
Else
	Global $sProfileDir = $sActiveFolder & _ProfileChoiceGui($aProfiles)
EndIf
_Aut2Log_Write($aProfileDirList, $__A2L_TYPE_ARRAY)

Global $hGui = GUICreate('BookTabEx', 600, 400)
GUISetBkColor(0xFFFFFF, $hGui)
Global $hProgress = GUICtrlCreateProgress(0, 0)
Global $hListView = GUICtrlCreateListView('Nome Libro|Cartella', 0, 0, 600, 300, -1, BitOR(0x00000004, 0x00000020, 0x00000300, 0x00010000))
; Same as BitOR ($LVS_EX_CHECKBOXES, $LVS_EX_FULLROWSELECT, $LVS_EX_GRIDLINES, $LVS_EX_DOUBLEBUFFER)
_GUICtrlListView_RegisterSortCallBack($hListView)
Global $TempListViewItem = GUICtrlCreateListViewItem('------|------', $hListView)
$aButton[0] = GUICtrlCreateButton('Analizza Cartella BooktabZ', 25, 335, 150, 30)
$aButton[1] = GUICtrlCreateButton('Decripta Libri Selezionati', 225, 335, 150, 30)
$aButton[2] = GUICtrlCreateButton('Seleziona Tutti', 390, 305, 100, 20)
$aButton[3] = GUICtrlCreateButton('Deseleziona Tutti', 495, 305, 100, 20)
$aButton[4] = GUICtrlCreateButton('Il libro manca nella lista', 425, 335, 150, 30)
$aButton[5] = GUICtrlCreateButton('About', 5, 305, 50, 20)
Global $hStatusBar = _GUICtrlStatusBar_Create($hGui)
_GUICtrlStatusBar_SetParts($hStatusBar, 3, $aStatusBarParts)
_GUICtrlStatusBar_SetText($hStatusBar, 'Pronto')
_GUICtrlStatusBar_EmbedControl($hStatusBar, 2, GUICtrlGetHandle($hProgress))
_GUICtrlStatusBar_SetMinHeight($hStatusBar, 20)
For $i = 1 To 4
	GUICtrlSetState($aButton[$i], $GUI_DISABLE)
Next
GUICtrlSetState($hListView, $GUI_DISABLE)
GUISetState(@SW_SHOW, $hGui)

While 1
	Local $nMsg = GUIGetMsg()
	Switch $nMsg
		Case $GUI_EVENT_CLOSE
			_GUICtrlListView_UnRegisterSortCallBack($hListView)
			_Aut2Log_Write('Gui closed', $__A2L_TYPE_MESSAGE)
			Exit
		Case $GUI_EVENT_RESTORE
			_GUICtrlStatusBar_EmbedControl($hStatusBar, 2, GUICtrlGetHandle($hProgress)) ; We have to redraw progressbar cause of an autoit "bug"
		Case $aButton[0]
			_GUICtrlListView_DeleteAllItems($hListView)
			Local $aBookList = _BookList($sProfileDir)
			If @error Then
				MsgBox(48, 'Errore', 'Non è presente alcun libro nella cartella di BookTabZ')
				_Aut2Log_Write('No books in BookTabZ folder. Damn...', $__A2L_TYPE_MESSAGE)
				ContinueLoop
			EndIf
			_Aut2Log_Write($aBookList, $__A2L_TYPE_ARRAY)
			Local $hListViewItem[UBound($aBookList)]
			For $i = 0 To UBound($aBookList) - 1
				$hListViewItem[$i] = GUICtrlCreateListViewItem($aBookList[$i][0] & "|" & $aBookList[$i][1], $hListView)
			Next
			_GUICtrlListView_SetColumnWidth($hListView, 0, $LVSCW_AUTOSIZE)
			_GUICtrlListView_SetColumnWidth($hListView, 1, $LVSCW_AUTOSIZE)
			For $i = 1 To 4
				GUICtrlSetState($aButton[$i], $GUI_ENABLE)
			Next
			GUICtrlSetState($hListView, $GUI_ENABLE)
		Case $aButton[1]
			Local $sPdfSavePath = FileSelectFolder('Salva nella cartella...', @DesktopDir, 3, @DesktopDir) & '\'
			If @error = 1 Then ContinueLoop
			_Aut2Log_Write('Save in: ' & $sPdfSavePath, $__A2L_TYPE_MESSAGE)
			Local $aSortedListView[_GUICtrlListView_GetItemCount($hListView)][2]
			For $i = 0 To UBound($aSortedListView) - 1
				Local $aTempGetListViewItemText = _GUICtrlListView_GetItemTextArray($hListView, $i)
				$aSortedListView[$i][0] = $aTempGetListViewItemText[1]
				$aSortedListView[$i][1] = $aTempGetListViewItemText[2]
			Next
			_Aut2Log_Write('Sorted array', $__A2L_TYPE_MESSAGE)
			_Aut2Log_Write($aSortedListView, $__A2L_TYPE_ARRAY)
			For $i = 0 To UBound($aSortedListView) - 1
				If _GUICtrlListView_GetItemChecked($hListView, $i) = True Then $iSelectedItems += 1
			Next
			_Aut2Log_Write('# of selected books: $iSelectedItems$', $__A2L_TYPE_MESSAGE)
			If $iSelectedItems = 0 Then
				MsgBox(48, 'Errore', 'Non è stato selezionato alcun libro')
				ContinueLoop
			EndIf
			For $i = 0 To 5
				GUICtrlSetState($aButton[$i], $GUI_DISABLE)
			Next
			GUICtrlSetState($hListView, $GUI_DISABLE)
			For $i = 0 To UBound($hListViewItem) - 1
				If _GUICtrlListView_GetItemChecked($hListView, $i) = False Then ContinueLoop
				$iCurrentBook += 1 ; This's here just for cosmetic purpouses
				Local $aXMLUnits = _XMLUnits($sProfileDir & '\' & $aSortedListView[$i][1] & '\volume.xml')
				_Aut2Log_Write('Units found in XML file (' & $sProfileDir & '\' & $aSortedListView[$i][1] & '\volume.xml', $__A2L_TYPE_MESSAGE)
				_Aut2Log_Write($aXMLUnits, $__A2L_TYPE_ARRAY)
				Local $aFolderUnits = _FolderUnits($sProfileDir & '\' & $aSortedListView[$i][1] & '\')
				If @error Then
					_Aut2Log_Write("Book not downloaded, skipping...", $__A2L_TYPE_MESSAGE)
					MsgBox(48, 'Errore', 'Il libro ' & $aSortedListView[$i][0] & ' non è stato scaricato in BookTabZ' & @CRLF & 'Dunque verrà ignorato')
					ContinueLoop
				EndIf
				_Aut2Log_Write("Units found in book's folder (" & $sProfileDir & '\' & $aSortedListView[$i][1] & '\', $__A2L_TYPE_MESSAGE)
				_Aut2Log_Write($aFolderUnits, $__A2L_TYPE_ARRAY)
				Local $aUnitSorted = _Sort2Arrays($aXMLUnits, $aFolderUnits)
				_Aut2Log_Write('Sorted units basing on XML', $__A2L_TYPE_MESSAGE)
				_Aut2Log_Write($aUnitSorted, $__A2L_TYPE_ARRAY)
				Local $aSwfPathList = _SwfPathList($sProfileDir & '\' & $aSortedListView[$i][1] & '\', $aUnitSorted)
				_Aut2Log_Write('SWF files path', $__A2L_TYPE_MESSAGE)
				_Aut2Log_Write($aSwfPathList, $__A2L_TYPE_ARRAY, False, $__LOG_OUTPUT, @CRLF)
				_GUICtrlStatusBar_SetText($hStatusBar, 'Estrazione capitoli libro ' & $iCurrentBook & ' su ' & $iSelectedItems)
				_Aut2Log_Write('GFX2GFX Parameters', $__A2L_TYPE_MESSAGE)
				For $in = 0 To UBound($aSwfPathList) - 1
					Local $hTimerBegin = TimerInit()
					Local $sGFX2GFXParameter = @ScriptDir & '\bin\SWFTools\gfx2gfx.exe "' & $aSwfPathList[$in] & '" "' & @ScriptDir & '\bin\SWFTools\Temp\' & $in + 1 & '.pdf"'
					_Aut2Log_Write($sGFX2GFXParameter, $__A2L_TYPE_PARAMETERS, False, $__LOG_OUTPUT, '|', @CRLF, @CRLF)
					RunWait($sGFX2GFXParameter, '', @SW_HIDE)
					GUICtrlSetData($hProgress, Int(($in + 1) / (UBound($aSwfPathList) - 1) * 100))
					#Region ### Time Remaining ; Thanks to Melba23 for his example script (http://goo.gl/zaFwr)
					Local $iThis_Time = Int(TimerDiff($hTimerBegin))
					$iCurr_Average = Int((($iCurr_Average * $in) + $iThis_Time) / ($in + 1))
					Local $iTime_Remaining = StringTrimRight((Int((UBound($aSwfPathList) - 1 - $in) * $iCurr_Average)), 3)
					If $iTime_Remaining <= 60 Then
						_GUICtrlStatusBar_SetText($hStatusBar, 'Circa ' & $iTime_Remaining & ' Secondi rimanenti', 1)
					Else
						Local $Minutes = Int($iTime_Remaining / 60)
						Local $Seconds = Mod($iTime_Remaining, 60)
						_GUICtrlStatusBar_SetText($hStatusBar, 'Circa ' & $Minutes & ' Minuto/i e ' & $Seconds & ' Secondi rimanenti', 1)
					EndIf
					#EndRegion ### Time Remaining ; Thanks to Melba23 for his example script (http://goo.gl/zaFwr)
				Next
				Local $sPdfTKParameter = '"' & @ScriptDir & '\bin\pdftk\pdftk.exe" '
				For $in = 1 To UBound($aSwfPathList)
					$sPdfTKParameter &= '"' & @ScriptDir & '\bin\SWFTools\Temp\' & $in & '.pdf" '
				Next
				$sPdfTKParameter &= 'output "' & $sPdfSavePath & $aSortedListView[$i][0] & '.pdf"'
				_Aut2Log_Write('PdfTKP Parameter', $__A2L_TYPE_MESSAGE)
				_Aut2Log_Write($sPdfTKParameter, $__A2L_TYPE_PARAMETERS)
				_GUICtrlStatusBar_SetText($hStatusBar, 'Potrebbe richiedere qualche minuto', 1)
				_GUICtrlStatusBar_SetText($hStatusBar, 'Sto unendo i capitoli...')
				RunWait($sPdfTKParameter, '', @SW_HIDE)
				_Aut2Log_Write('Deleting temp .pdf files', $__A2L_TYPE_MESSAGE)
				For $in = 1 To UBound($aSwfPathList)
					Local $sTempFileToDelete = @ScriptDir & '\bin\SWFTools\Temp\' & $in & '.pdf'
					FileDelete($sTempFileToDelete)
					_Aut2Log_Write($sTempFileToDelete & ' Deleted', $__A2L_TYPE_MESSAGE, False, $__LOG_OUTPUT, '|', @CRLF, @CRLF)
				Next
				GUICtrlSetData($hProgress, 0)
			Next
			_Aut2Log_Write('Done', $__A2L_TYPE_MESSAGE)
			_GUICtrlStatusBar_SetText($hStatusBar, 'Pronto')
			_GUICtrlStatusBar_SetText($hStatusBar, '', 1)
			For $i = 0 To 5
				GUICtrlSetState($aButton[$i], $GUI_ENABLE)
			Next
			GUICtrlSetState($hListView, $GUI_ENABLE)
			Global $iSelectedItems = 0, $iCurrentBook = 0, $iCurr_Average = 0
		Case $aButton[2]
			For $i = 0 To UBound($hListViewItem) - 1
				GUICtrlSetState($hListViewItem[$i], $GUI_CHECKED)
			Next
		Case $aButton[3]
			For $i = 0 To UBound($hListViewItem) - 1
				GUICtrlSetState($hListViewItem[$i], $GUI_UNCHECKED)
			Next
		Case $aButton[4]
			For $i = 0 To 5
				GUICtrlSetState($aButton[$i], $GUI_DISABLE)
			Next
			GUICtrlSetState($hListView, $GUI_DISABLE)
			If Not ProcessExists('BooktabZ.exe') Then
				FileDelete('$sProfileDir$\tmp\*.')
				_Aut2Log_Write('Tmp folder emptied', $__A2L_TYPE_MESSAGE)
			Else
				MsgBox(48, 'Errore', 'BookTabZ al momento è in esecuzione.@CRLF@Chiudere il programma e riprovare.')
				GUICtrlSetState($aButton[0], $GUI_ENABLE)
				ContinueLoop
			EndIf
			_GUICtrlStatusBar_SetText($hStatusBar, 'In attesa che venga aperto BookTabZ')
			ProcessWait('BooktabZ.exe')
			_GUICtrlStatusBar_SetText($hStatusBar, 'In attesa del completamento')
			MsgBox(64 + 262144, 'In attesa', "Visulizzare in BookTabZ uno dopo l'altro tutti i capitoli@CRLF@del libro che si vuole estrarre e cliccare su Ok al termine.")

			Local $aChptrsLst = _Mthd2PDFChptrs('$sProfileDir$\tmp\')
			_Aut2Log_Write('Chapters sorted list:', $__A2L_TYPE_MESSAGE)
			_Aut2Log_Write($aChptrsLst, $__A2L_TYPE_ARRAY)

			_GUICtrlStatusBar_SetText($hStatusBar, 'Salvando il pdf')
			#Region File Save
			Local $sPdfTKParameter = '"@ScriptDir@\bin\pdftk\pdftk.exe" '
			For $i = 0 To UBound($aChptrsLst) - 1
				$sPdfTKParameter &= '"' & $aChptrsLst[$i][0] & '" '
			Next

			Local $hFlSvDlg = FileSaveDialog('Salva come...', @DesktopDir, '(File PDF) *.pdf')
			If @error Then
				ContinueLoop
			ElseIf StringRight($hFlSvDlg, 4) <> '.pdf' Then
				$hFlSvDlg &= '.pdf'
			EndIf

			$sPdfTKParameter &= 'output "$hFlSvDlg$"'
			_Aut2Log_Write('PdfTKP Parameter', $__A2L_TYPE_MESSAGE)
			_Aut2Log_Write($sPdfTKParameter, $__A2L_TYPE_MESSAGE)
			RunWait($sPdfTKParameter, '', @SW_HIDE)
			#EndRegion File Save
			ProcessClose('BooktabZ.exe')
			_GUICtrlStatusBar_SetText($hStatusBar, 'Pronto')
			GUICtrlSetState($aButton[0], $GUI_ENABLE)
		Case $aButton[5]
			_AboutGui()
		Case $hListView
			_GUICtrlListView_SortItems($hListView, GUICtrlGetState($hListView))
	EndSwitch
WEnd

#Region ### Functions ###
; #FUNCTION# ====================================================================================================================
; Name...........:	_BookList()
; Description ...:	Retrieve units list in XML file
; Syntax.........:	_XMLUnits($sPath)
; Parameters ....:	$sPath				- Path to XML
; Return values .: 	On Success			- Returns an array with the list of units found in the file
;                 	On Failure			- @error and @extended according to RegEx reference, and
;											5 = Can't open specified file
; ===============================================================================================================================
Func _BookList($sDir)
	Local $aTempBookDirList = __FileListToArray($sDir, '*', 2)
	If @error = 2 Or UBound($aTempBookDirList) < 4 Then Return SetError(1)
	Local $aBookDirList[UBound($aTempBookDirList) - 3], $aBookTitle[UBound($aTempBookDirList) - 3], $aBooksArray[0][2]
	_Aut2Log_Write('Profile folder content', $__A2L_TYPE_MESSAGE)
	_Aut2Log_Write($aTempBookDirList, $__A2L_TYPE_ARRAY)
	For $i = 0 To UBound($aTempBookDirList) - 4
		$aBookDirList[$i] = $aTempBookDirList[$i]
	Next
	_ArrayDisplay($aBookDirList)
	_Aut2Log_Write('Successfully removed non-book folders', $__A2L_TYPE_MESSAGE)
	For $i = 0 To UBound($aBookDirList) - 1


		Local $hVolumeXMLOpen = FileOpen($sDir & '\' & $aBookDirList[$i] & '\Volume.xml')
		Local $sVolumeXMLRead = FileRead($hVolumeXMLOpen)
		FileClose($hVolumeXMLOpen)
		Local $aRegexTitle = StringRegExp($sVolumeXMLRead, '(?s)<volumetitle>(.*)</volumetitle>', 3)
		If IsArray($aRegexTitle) = False Then
			$aBookTitle[$i] = Null
			ContinueLoop
		EndIf
		Local $sBookTitle = StringStripCR($aRegexTitle[0])
		$sBookTitle = StringReplace($sBookTitle, '<![CDATA[', '')
		$sBookTitle = StringReplace($sBookTitle, ']]>', '')
		$aBookTitle[$i] = StringStripWS($sBookTitle, 3)
	Next
	For $i = 0 To UBound($aBookDirList) - 1
		If $aBookTitle[$i] = Null Then ContinueLoop
		ReDim $aBooksArray[UBound($aBooksArray) + 1][2]
		$aBooksArray[UBound($aBooksArray) - 1][0] = $aBookTitle[$i]
		$aBooksArray[UBound($aBooksArray) - 1][1] = $aBookDirList[$i]
	Next
	Return $aBooksArray
EndFunc   ;==>_BookList

; #FUNCTION# ====================================================================================================================
; Name...........:	_XMLUnits()
; Description ...:	Retrieve units list in XML file
; Syntax.........:	_XMLUnits($sPath)
; Parameters ....:	$sPath				- Path to XML
; Return values .: 	On Success			- Returns an array with the list of units found in the file
;                 	On Failure			- @error and @extended according to RegEx reference, and
;											5 = Can't open specified file
; ===============================================================================================================================
Func _XMLUnits($sPath)
	Local $hFileOpen = FileOpen($sPath)
	If $hFileOpen = -1 Then Return SetError(5)
	Local $sFileRead = FileRead($hFileOpen)
	FileClose($hFileOpen)
	Local $aRegEx = StringRegExp($sFileRead, '<unit href="(.*).zip"', 3)
	If @error Then Return SetError(@error, @extended)
	Return $aRegEx
EndFunc   ;==>_XMLUnits

; #FUNCTION# ====================================================================================================================
; Name...........:	_FolderUnits()
; Description ...:	Retrieve units list in XML file
; Syntax.........:	_FolderUnits($sDir)
; Parameters ....:	$sDir				- Book's directory
; Return values .: 	On Success			- Returns an array with the list of units found in the directory
;                 	On Failure			- @error
;											1 = Nothing found
; ===============================================================================================================================
Func _FolderUnits($sDir)
	Local $sFolderList = Null, $hFileFindFirst = FileFindFirstFile($sDir & '*')
	While 1
		Local $hFileFindNext = FileFindNextFile($hFileFindFirst)
		If @error Then
			ExitLoop
		ElseIf @extended = 1 Then ; If file found is a folder
			$sFolderList &= $hFileFindNext & '|'
		EndIf
	WEnd
	FileClose($hFileFindFirst)
	If $sFolderList = Null Then Return SetError(1)
	$sFolderList = StringReplace($sFolderList, 'InternalAssets', '') ; Remove useless folders from list
	$sFolderList = StringReplace($sFolderList, 'images', '') ; Remove useless folders from list
	If StringRight($sFolderList, 1) = '|' Then $sFolderList = StringTrimRight($sFolderList, 1)
	Return StringSplit($sFolderList, '|', 2)
EndFunc   ;==>_FolderUnits

; #FUNCTION# ====================================================================================================================
; Name...........:	_Sort2Arrays()
; Description ...:	Sort elements in 1st array basing on 2nd pattern (if present)
; Syntax.........:	_Sort2Arrays(ByRef $aArray1, ByRef $aArray2)
; Parameters ....:	$aArray1			- "Mother" array
;					$aArray2			- "Daughter" array
; Return values .: 	On Success			- Elements present in Array2 sorted
;                 	On Failure			- No error handling
; ===============================================================================================================================
Func _Sort2Arrays(ByRef $aArray1, ByRef $aArray2)
	Local $aTempArray[0]
	For $i = 0 To UBound($aArray1) - 1
		Local $iSearch = __SearchInArray($aArray2, $aArray1[$i])
		If @error Then ContinueLoop
		ReDim $aTempArray[UBound($aTempArray) + 1]
		$aTempArray[UBound($aTempArray) - 1] = $aArray1[$i]
	Next
	Return $aTempArray
EndFunc   ;==>_Sort2Arrays

; #FUNCTION# ====================================================================================================================
; Name...........:	_SwfPathList()
; Description ...:	Returns the path to all swf chapters of a book
; Syntax.........:	_SwfPathList($Dir, ByRef $aSubFolderArray)
; Parameters ....:	$Dir				- Book's folder path
;					$aSubFolderArray	- Zero-based array of book's chapters subfolders
; Return values .: 	On Success			- Returns 1 dimensional zero-based array.
;											1st dimension: Full file path
;                 	On Failure			- No error handling
; ===============================================================================================================================
Func _SwfPathList($Dir, ByRef $aSubFolderArray)
	Local $sSwfPathList = Null
	For $i = 0 To UBound($aSubFolderArray) - 1
		Local $hFileFindFirst = FileFindFirstFile($Dir & $aSubFolderArray[$i] & '\assets\pages\*.swf')
		Local $hFileFindNext = FileFindNextFile($hFileFindFirst)
		If Not @error Then
			$sSwfPathList &= $Dir & $aSubFolderArray[$i] & '\assets\pages\$hFileFindNext$|'
		EndIf
		FileClose($hFileFindFirst)
	Next
	If $sSwfPathList = Null Then Return SetError(1)
	$sSwfPathList = StringRegExpReplace($sSwfPathList, '\|+', '|') ;CHECK
	If StringRight($sSwfPathList, 1) = '|' Then $sSwfPathList = StringTrimRight($sSwfPathList, 1)
	Return StringSplit($sSwfPathList, '|', 2)
EndFunc   ;==>_SwfPathList

; #FUNCTION# ====================================================================================================================
; Name...........:	_Mthd2PDFChptrs()
; Description ...:	2nd Method for PDF extraction, merging ready-made files
; Syntax.........:	_Mthd2PDFChptrs($sPath)
; Parameters ....:	$sPath				- Path to tmp folder inside BookTabZ's folder
; Return values .: 	On Success			- Returns sorted 2 dimensional zero-based array.
;											1st dimension: Full file path
;											2nd dimension: File timestamp
;                 	On Failure			- @error set to:
;											1 = No DOC**** files found in specified folder
; ===============================================================================================================================
Func _Mthd2PDFChptrs($sPath)
	Local $aDOCLst = __FileListToArray($sPath, 'DOC*', 1)
	Local $aChptrsLst[UBound($aDOCLst)][2]
	For $i = 0 To UBound($aChptrsLst) - 1
		$aChptrsLst[$i][0] = $aDOCLst[$i]
		$aChptrsLst[$i][1] = FileGetTime($sPath & $aDOCLst[$i], 0, 1)
	Next
	_ArraySort($aChptrsLst, 0, 0, 0, 1)
	Return $aChptrsLst
EndFunc   ;==>_Mthd2PDFChptrs

; #FUNCTION# ====================================================================================================================
; Name...........:	_ProfileChoiceGui
; Description ...:	A function wrapping gui creation and related stuff
; Syntax.........:	_ProfileChoiceGui ($sFolder, ByRef $aProfiles)
; Parameters ....:	$sFolder			- BookTab's profiles folder
;					$aProfiles			- Array with found profiles
; Return values .: 	On Success			Returns chosen profile name
;                 	On Failure			- No error handling
; ===============================================================================================================================
Func _ProfileChoiceGui(ByRef $aProfiles)
	Local $hGui = GUICreate('Scegliere il profilo', 400, 400), $aRadios[UBound($aProfiles)]
	GUISetBkColor(0xFFFFFF, $hGui)
	GUIStartGroup($hGui)
	For $i = 0 To UBound($aProfiles) - 1
		$aRadios[$i] = GUICtrlCreateRadio($aProfiles[$i], 20, 30 + 30 * $i, 300, 20)
	Next
	GUICtrlSetState($aRadios[0], 1)
	Local $hConfirmBtn = GUICtrlCreateButton('Conferma', 165, 350, 70, 30)
	GUISetState(@SW_SHOW)
	While 1
		Local $nMsg = GUIGetMsg()
		Switch $nMsg
			Case $GUI_EVENT_CLOSE
				_Aut2Log_Write('Profile gui closed', $__A2L_TYPE_MESSAGE)
				Exit
			Case $hConfirmBtn
				For $i = 0 To UBound($aProfiles) - 1
					If GUICtrlRead($aRadios[$i]) = $GUI_CHECKED Then
						GUIDelete($hGui)
						_Aut2Log_Write('Profile chosen:' & $aProfiles[$i], $__A2L_TYPE_MESSAGE)
						Return $aProfiles[$i]
					EndIf
				Next
				MsgBox(48, 'Errore', 'Non è stato scelto alcun profilo')
		EndSwitch
	WEnd
EndFunc   ;==>_ProfileChoiceGui

