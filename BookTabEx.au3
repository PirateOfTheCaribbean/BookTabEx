; Name...........:	_AboutGui()
; Description ...:	A function wrapping gui creation and related stuff
; Syntax.........:	_AboutGui()
; Parameters ....:	-
; Return values .: 	On Success			-
;                 	On Failure			- No error handling
; ===============================================================================================================================
Func _AboutGui()
	GUISetState(@SW_DISABLE, $hGui)

	Global $hAboutGui = GUICreate('About', 300, 150, -1, -1, -1, -1, $hGui)
	GUISetBkColor(0xFFFFFF, $hAboutGui)

	Global $binChipsound = _XM_Sound()
	Global $tMem = DllStructCreate("byte[" & BinaryLen($binChipsound) & "]")
	DllStructSetData($tMem, 1, $binChipsound)
	Global $hBassDLL_x64 = MemoryDllOpen(_BassDLL_x64())
	_BASS_Init(0)
	Global $hMusicHandle = _BASS_MusicLoad(True, DllStructGetPtr($tMem), 0, BinaryLen($binChipsound), $BASS_SAMPLE_LOOP, 0)
	_BASS_ChannelPlay($hMusicHandle, 1)
	GUISetState(@SW_SHOW, $hAboutGui)

	_GDIPlus_Startup()

	Local $hLabelGraphic = _GDIPlus_GraphicsCreateFromHWND($hAboutGui)
	Local $hLabelBrush = _GDIPlus_BrushCreateSolid()
	Local $hLabelFormat = _GDIPlus_StringFormatCreate()
	_GDIPlus_StringFormatSetAlign($hLabelFormat, 1)
	Local $hFamily = _GDIPlus_FontFamilyCreate("Arial")
	Local $hFont = _GDIPlus_FontCreate($hFamily, 12)
	Local $tLayout = _GDIPlus_RectFCreate(10, 92, 280, 40)
	_GDIPlus_GraphicsDrawStringEx($hLabelGraphic, 'A big thank you to BookTabZ for their handy temporary pdf folder', $hFont, $tLayout, $hLabelFormat, $hLabelBrush)

	Local $hImageGraphics = _GDIPlus_GraphicsCreateFromHWND($hAboutGui)
	Local $hBitmap = _GDIPlus_BitmapCreateFromMemory(_fede97())
	$hBitmap = _GDIPlus_ImageResize($hBitmap, 230, 70)
	Local $hPic = _GDIPlus_GraphicsDrawImage($hImageGraphics, $hBitmap, 35, 5)

	_GDIPlus_GraphicsDispose($hLabelGraphic)
	_GDIPlus_GraphicsDispose($hImageGraphics)
	_GDIPlus_BrushDispose($hLabelBrush)
	_GDIPlus_StringFormatDispose($hLabelFormat)
	_GDIPlus_FontFamilyDispose($hFamily)
	_GDIPlus_FontDispose($hFont)
	_GDIPlus_ImageDispose($hBitmap)

	_GDIPlus_Shutdown()

	GUIRegisterMsg($WM_NCHITTEST, "__AboutGuiDrag")

	While 1
		Local $nMsg = GUIGetMsg()
		Switch $nMsg
			Case $GUI_EVENT_CLOSE
				GUIRegisterMsg($WM_NCHITTEST, "")
				__BassClose()
				GUISetState(@SW_ENABLE, $hGui)
				GUIDelete($hAboutGui)
				Return
		EndSwitch
	WEnd
EndFunc   ;==>_AboutGui

; #INTERNAL_USE_ONLY# ===========================================================================================================
; Name...........:	__FileListToArray()
; Description ...:	Lists files and\or folders in a specified folder. Similar to _FileListToArray, but as I need it
; Syntax.........:	__FileListToArray($sFilePath, $sFilter, $iFlag, $bReturnPath)
; Parameters ....:	$sFilePath			- Folder to generate filelist for.
;										- [optional] The filter to use, default is *
;					$iFlag				- [optional] Specifies whether to return files folders or both
;											0 = (Default) Return both files and folders
;											1 = Return files only
;											2 = Return folders only
;					$bReturnPath		- [optional] If True the full path is appended to the file\folder path, otherwise it is
;										  relative to the $sFilePath folder. Default is False.
; Return values .: 	On Success			- Returns a 0-based array with all the elements found
;                 	On Failure			- @error set to:
;											1 = Specified directory does not exists
;											2 = Nothing found
; ===============================================================================================================================
Func __FileListToArray($sFilePath, $sFilter = "*", $iFlag = 0, $bReturnPath = False)
	If StringRight($sFilePath, 1) <> '\' Then $sFilePath &= '\'
	If Not FileExists($sFilePath) Then Return SetError(1)

	Local $sSearchList = Null, $hFileFindFirst = FileFindFirstFile($sFilePath & $sFilter)
	While 1
		Local $hFileFindNext = FileFindNextFile($hFileFindFirst)
		If @error Then
			ExitLoop
		ElseIf $iFlag = 0 Then
			$sSearchList &= ($bReturnPath = True) ? ($sFilePath & $hFileFindNext & '|') : ($hFileFindNext & '|')
		ElseIf $iFlag > 0 And @extended = $iFlag - 1 Then ; @extended = 0 if file, 1 if folder
			$sSearchList &= ($bReturnPath = True) ? ($sFilePath & $hFileFindNext & '|') : ($hFileFindNext & '|')
		EndIf
	WEnd
	FileClose($hFileFindFirst)
	If $sSearchList = Null Then Return SetError(2)
	$sSearchList = StringTrimRight($sSearchList, 1)

	Return StringSplit($sSearchList, '|', 2)
EndFunc   ;==>__FileListToArray

; #INTERNAL_USE_ONLY# ===========================================================================================================
; Name...........:	__SearchInArray()
; Description ...:	Search for a string inside and array
; Syntax.........:	__SearchInArray(ByRef $aArray, $sString)
; Parameters ....:	$aArray				- Array to search in
;					$sString			- Substring to search
; Return values .: 	On Success			- Returns an integer representing position of the string inside the array
;                 	On Failure			- @error set to:
;											1 = String not found
; ===============================================================================================================================
Func __SearchInArray(ByRef $aArray, $sString)
	For $i = 0 To UBound($aArray) - 1
		If $aArray[$i] = $sString Then Return $i
	Next
	Return SetError(1)
EndFunc   ;==>__SearchInArray

